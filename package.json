{
  "name": "@inshel/template-node",
  "version": "0.1.0",
  "description": "InShel node template",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf ./target && babel ./src --out-dir ./target",
    "test": "jest",
    "pre-dev": "babel-node ./scripts/pre-dev.js",
    "new-contract": "babel-node ./scripts/new-contract.js",
    "key:info": "babel-node ./scripts/info.js",
    "invite": "babel-node ./scripts/invite.js",
    "lint:fix": "npx standard --fix"
  },
  "dependencies": {
    "@inshel/node": "^0.1.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-regenerator": "^7.4.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "core-js": "^3.1.4",
    "husky": "^3.0.2",
    "jest": "^24.8.0",
    "lint-staged": "^9.2.1",
    "node-jsencrypt": "^1.0.0",
    "standard": "^13.1.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-transform-regenerator"
    ]
  },
  "jest": {
    "bail": 1,
    "maxConcurrency": 1,
    "verbose": true,
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.test.js$",
    "transform": {
      "js": "babel-jest"
    }
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "git add"
    ]
  },
  "keywords": [
    "InShel",
    "node",
    "node-template"
  ],
  "author": "Dmitry <db@inshel.com> Balakov",
  "license": "ISC"
}
